{
  "name": "blimp",
  "version": "1.0.0",
  "description": "An interactive dashboard service. ",
  "main": "entry.js",
  "engines": {
    "node": "10.15.*"
  },
  "dependencies": {
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "opener": "^1.5.1",
    "pg": "^7.12.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^10.2.0",
    "nsp": "^3.2.1",
    "nyc": "^14.1.0",
    "rimraf": "^2.6.3"
  },
  "nyc": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": true,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "scripts": {
    "start": "node source/entry.js",
    "clean": "rimraf .nyc_output coverage",
    "cov": "opener coverage/lcov-report/index.html",
    "test": "npm run clean && nyc mocha source/test/",
    "test-only": "npm run clean && nyc mocha source/test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blimp-live/blimp.git"
  },
  "author": "blimp-live",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blimp-live/blimp/issues"
  },
  "homepage": "https://github.com/blimp-live/blimp#readme"
}
